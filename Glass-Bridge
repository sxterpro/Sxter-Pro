-- Load UI Library
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/x2zu/OPEN-SOURCE-UI-ROBLOX/refs/heads/main/X2ZU%20UI%20ROBLOX%20OPEN%20SOURCE/DummyUi-leak-by-x2zu/fetching-main/Tools/Framework.luau"))()

-- Create Main Window
local Window = Library:Window({
    Title = "Sxter Pro",
    Desc = "By Sxter",
    Icon = 105059922903197,
    Theme = "Dark",
    Config = {
        Keybind = Enum.KeyCode.LeftControl,
        Size = UDim2.new(0, 500, 0, 400)
    },
    CloseUIButton = {
        Enabled = true,
        Text = "Sxter"
    }
})

-- Sidebar Vertical Separator
local SidebarLine = Instance.new("Frame")
SidebarLine.Size = UDim2.new(0, 1, 1, 0)
SidebarLine.Position = UDim2.new(0, 140, 0, 0) -- adjust if needed
SidebarLine.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
SidebarLine.BorderSizePixel = 0
SidebarLine.ZIndex = 5
SidebarLine.Name = "SidebarLine"
SidebarLine.Parent = game:GetService("CoreGui") -- Or Window.Gui if accessible

-- Tab
local Tab = Window:Tab({Title = "Main", Icon = "star"}) do
    -- Section
    Tab:Section({Title = "Teleport"})

    -- Button
    Tab:Button({
        Title = "Teleport",
        Desc = "Click to Teleport To Win",
        Callback = function()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-8.52859592, 44.3852463, 503.717224, 1, 0, 0, 0, 1, 0, 0, 0, 1)
            Window:Notify({
                Title = "Button",
                Desc = "Action performed successfully.",
                Time = 3
            })
        end
    })
    
Tab:Button({
        Title = "กด E ทีเดียว",
        Desc = "Click to one press E",
        Callback = function()
            local ws = workspace
local fixedCount = 0

-- ฟังก์ชันสำหรับแก้ค่า ProximityPrompt
local function fixPrompt(obj)
    if obj:IsA("ProximityPrompt") then
        obj.HoldDuration = 0
        fixedCount += 1
        print("[ProximityFix] Fixed ProximityPrompt:", obj:GetFullName(), "| Total fixed:", fixedCount)
    end
end

-- สแกนทั้งหมดใน workspace ตอนเริ่ม
for _, descendant in ipairs(ws:GetDescendants()) do
    fixPrompt(descendant)
end

-- คอยเฝ้า ถ้ามีของใหม่เพิ่มเข้ามาใน workspace
ws.DescendantAdded:Connect(function(obj)
    fixPrompt(obj)
end)
            Window:Notify({
                Title = "Button",
                Desc = "Action performed successfully.",
                Time = 3
            })
        end
    })
    -- Simulate update
    task.delay(5, function()
        CodeBlock:SetCode("-- Updated!\nprint('New content loaded')")
    end)
end

-- Line Separator
Window:Line()

local Extra = Window:Tab({Title = "Settings", Icon = "wrench"}) do
    Extra:Section({Title = "Credit"})
    Extra:Button({
        Title = "Credit By Sxter",
        Desc = "Thank !!",
        Callback = function()
            Window:Notify({
                Title = "By Sxter",
                Desc = "Thank You !!",
                Time = 3
            })
        end
    })
end
Extra:Button({
        Title = "Join Discord",
